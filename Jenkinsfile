pipeline {
    agent any

    stages {
        stage ('Build'){
            steps {
                echo "Building..."
                sh 'python3 --version'

            }
        }

        stage ('Test'){
            steps {
                echo 'Testing'
                sh 'python3 --version'
            }

        }
         stage ('Notifications'){
            steps {
                echo 'Notifications'
                sh 'python3 --version'
            }

        }


    }
}